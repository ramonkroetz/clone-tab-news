{
  "name": "clone-tab-news",
  "version": "1.0.0",
  "description": "## NVM Version (.nvmrc) nvm install",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "migration:create": "node-pg-migrate -m infra/migrations -j sql create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development --language ts up",
    "services:down": "docker compose -f infra/compose.yaml down",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand",
    "typecheck": "tsc"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.4",
    "dotenv-expand": "^11.0.6",
    "next": "^13.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2"
  }
}
